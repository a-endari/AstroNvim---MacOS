        -- Words to exclude from -ign to -ing conversion
        local ign_exclusions = {"align", "assign", "campaign", "cosign", "design", "feign", "foreign", "reign", "sign",
                                "sovereign"}

        -- Create abbreviations for all corrections
        for wrong, right in pairs(common_corrections) do
            vim.cmd(string.format('iabbrev %s %s', wrong, right))
        end

        -- Function to fix text formatting
        local function fix_text_formatting()
            if not vim.bo.modifiable then
                return
            end

            local line = vim.api.nvim_get_current_line()
            local modified_line = line

            -- Fix spacing around punctuation
            modified_line = modified_line:gsub("%s+([%.,%?!:;])", "%1")
            modified_line = modified_line:gsub("([%.,%?!:;])(%S)", "%1 %2")
            modified_line = modified_line:gsub("%s+", " ")
            modified_line = modified_line:gsub("^%s+", "")

            -- Apply word ending corrections
            for wrong, right in pairs(word_endings) do
                modified_line = modified_line:gsub(wrong .. "(%s)", right .. "%1")
            end

            -- Apply word beginning corrections
            for wrong, right in pairs(word_beginnings) do
                modified_line = modified_line:gsub("(%s)" .. wrong, "%1" .. right)
                modified_line = modified_line:gsub("^" .. wrong, right)
            end

            -- Handle -ign to -ing conversion (excluding specific words)
            local function should_convert_ign(word)
                for _, excluded in ipairs(ign_exclusions) do
                    if word == excluded then
                        return false
                    end
                end
                return true
            end

            modified_line = modified_line:gsub("(%w+)ign(%s)", function(prefix, suffix)
                if should_convert_ign(prefix .. "ign") then
                    return prefix .. "ing" .. suffix
                end
                return prefix .. "ign" .. suffix
            end)

            -- Capitalize after sentence endings
            modified_line = modified_line:gsub("([.!?])%s+(%a)", function(punct, letter)
                return punct .. " " .. letter:upper()
            end)

            -- Capitalize I's
            modified_line = modified_line:gsub("%si%s", " I ")
            modified_line = modified_line:gsub("^i%s", "I ")

            -- Check for start of line capitalization
            if modified_line:match("^%s*%l") then
                modified_line = modified_line:gsub("^%s*%l", string.upper)
            end

            -- Only update if changes were made
            if modified_line ~= line then
                vim.api.nvim_set_current_line(modified_line)
            end
        end

        -- Set up autocommands
        vim.api.nvim_create_autocmd({"InsertLeave", "TextChanged", "TextChangedI"}, {
            pattern = "*",
            callback = function()
                if vim.bo.modifiable then
                    fix_text_formatting()
                end
            end
        })

        -- First letter capitalization for new files
        vim.api.nvim_create_autocmd("BufEnter", {
            pattern = "*",
            callback = function()
                if not vim.bo.modifiable then
                    return
                end
                local first_line = vim.api.nvim_buf_get_lines(0, 0, 1, false)[1]
                if first_line and first_line:match("^%l") then
                    local modified_line = first_line:gsub("^%l", string.upper)
                    vim.api.nvim_buf_set_lines(0, 0, 1, false, {modified_line})
                end
            end
        })

        return opts
    end
